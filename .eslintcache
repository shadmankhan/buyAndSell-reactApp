 {toUpper(sellerName.charAt(0))}\r\n            </Avatar>\r\n          }\r\n          action={\r\n            <IconButton aria-label='settings'>\r\n              <MoreVertIcon />\r\n            </IconButton>\r\n          }\r\n          title={sellerName}\r\n          subheader={postedOn}\r\n        />\r\n        {specialRibbon && (\r\n          <div className={`${classes.ribbon} ${classes.ribbonTopRight}`}>\r\n            <Typography className={classes.ribbonText}>\r\n              DISCOUNT&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n            </Typography>\r\n          </div>\r\n        )}\r\n        <CardMedia\r\n          className={classes.media}\r\n          image={productImage}\r\n          // title={productTitle}\r\n        />\r\n        <CardContent>\r\n          <Tooltip title={productTitle}>\r\n            <Typography variant='body2'>\r\n              {productTitle.length > 80\r\n                ? `${productTitle.slice(0, 80)}...`\r\n                : productTitle}\r\n            </Typography>\r\n          </Tooltip>\r\n          {productTitle.length < 45 ? <br /> : \"\"}\r\n          <br />\r\n          <Grid container alignItems='baseline'>\r\n            <Grid item>\r\n              <Typography variant='body1'>M.R.P.:&nbsp;</Typography>\r\n            </Grid>\r\n            <Grid item>\r\n              <Typography variant='body1' color='error'>\r\n                <strike>{costPrice}</strike>\r\n              </Typography>\r\n            </Grid>\r\n          </Grid>\r\n          <Grid container alignItems='baseline'>\r\n            <Grid item>\r\n              <Typography variant='body1'>Price:&nbsp;</Typography>\r\n            </Grid>\r\n            <Grid item>\r\n              <Typography\r\n                variant='body1'\r\n                className={classes.fW500}\r\n                color='error'\r\n              >\r\n                {sellingPrice}\r\n              </Typography>\r\n            </Grid>\r\n            <Grid item>\r\n              <Typography variant='caption'>\r\n                &nbsp;FREE Scheduled Delivery\r\n              </Typography>\r\n            </Grid>\r\n          </Grid>\r\n        </CardContent>\r\n\r\n        <CardActions disableSpacing>\r\n          {quantity > 0 ? (\r\n            <Grid container>\r\n              <Box px={-10}>\r\n                <Button\r\n                  className={classes.decBtn}\r\n                  onClick={() => removeFromCart(product)}\r\n                >\r\n                  -\r\n                </Button>\r\n              </Box>\r\n              <Typography className={classes.cartCount}>{quantity}</Typography>\r\n              <Box px={-10}>\r\n                <Button\r\n                  className={classes.incBtn}\r\n                  onClick={() => addToCart(product)}\r\n                  disabled={quantity >= limit ? true : false}\r\n                >\r\n                  +\r\n                </Button>\r\n              </Box>\r\n            </Grid>\r\n          ) : (\r\n            <Button\r\n              onClick={(e) => {\r\n                e && e.stopPropagation() && e.stopImmediatePropagation();\r\n                addToCart(product);\r\n              }}\r\n              variant='outlined'\r\n              className={classnames(classes.addedToCartBtn)}\r\n            >\r\n              <Typography variant='button'>{\"Add to Cart\"}</Typography>\r\n            </Button>\r\n          )}\r\n\r\n          {/* <IconButton\r\n            className={classnames(classes.expand, {\r\n              [classes.expandOpen]: expanded,\r\n            })}\r\n            onClick={this.handleExpandClick}\r\n            aria-expanded={expanded}\r\n            aria-label=\"show more\"\r\n          >\r\n            <ExpandMoreIcon />\r\n          </IconButton> */}\r\n          <IconButton onClick={this.handleOpen} className={classes.iconBtn}>\r\n            <AcUnitIcon />\r\n          </IconButton>\r\n          <ModalComponent isOpen={isModalOpen} 